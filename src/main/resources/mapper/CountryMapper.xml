<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javaclimb.drug.mapper.CountryMapper">


    <select id="getCountryName" resultType="java.lang.String">
        select distinct country_name from country
    </select>
    <select id="getCountryList" resultType="com.javaclimb.drug.entity.Country">
        select * from country
    </select>


</mapper>